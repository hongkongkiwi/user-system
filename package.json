{
  "name": "fastify-user",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "start": "npm run build:ts && fastify start -l info dist/src/app.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/src/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^10.1.0",
    "@fastify/auth": "^4.2.0",
    "@fastify/autoload": "^5.0.0",
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.2.0",
    "@fastify/sensible": "^5.0.0",
    "@fastify/swagger": "^8.3.1",
    "@fastify/swagger-ui": "^1.5.0",
    "@zenstackhq/runtime": "^1.0.0-alpha.60",
    "bcrypt": "^5.1.0",
    "class-validator": "^0.14.0",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.7.1",
    "fastify-graceful-shutdown": "^3.4.0",
    "fastify-plugin": "^4.0.0",
    "friendly-username-generator": "^2.0.4",
    "isemail": "^3.2.0",
    "moment": "^2.29.4",
    "oso": "^0.26.4",
    "request-ip": "^3.3.0",
    "superstruct": "^1.0.3"
  },
  "devDependencies": {
    "@casl/ability": "^6.3.3",
    "@casl/prisma": "^1.3.3",
    "@fastify/type-provider-json-schema-to-ts": "^2.2.2",
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^18.14.6",
    "@types/tap": "^15.0.5",
    "concurrently": "^7.0.0",
    "fastify-cli": "^5.7.1",
    "fastify-tsconfig": "^1.0.1",
    "glob": "^9.2.1",
    "prisma": "^4.11.0",
    "prisma-dbml-generator": "^0.10.0",
    "prisma-docs-generator": "^0.6.0",
    "prisma-enum-generator": "^1.0.3",
    "prisma-json-schema-generator": "^3.1.2",
    "tap": "^16.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "zenstack": "^1.0.0-alpha.60"
  }
}
